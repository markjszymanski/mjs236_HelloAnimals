package edu.pitt.helloanimals;

public class Mammal extends Animal {

	public Mammal(String age, String color, int weight, int minimumWeight) {
		super(age, color, weight, minimumWeight);
		type = "Mammal";
	}
	
	public int run() {
		return (getWeight()-1);
	}
}
