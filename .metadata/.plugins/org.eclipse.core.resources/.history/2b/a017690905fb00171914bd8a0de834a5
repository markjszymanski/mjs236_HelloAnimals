package edu.pitt.helloanimals;

public class Mammal extends Animal {

	public Mammal(String age, String color, String type, int weight, int minimumWeight) {
		super.Animal(age, color, type, weight, minimumWeight);
	}
	
	public int run() {
		return (getWeight()-1);
	}
}
