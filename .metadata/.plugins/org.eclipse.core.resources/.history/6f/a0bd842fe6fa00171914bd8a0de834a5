package edu.pitt.helloanimals;

public class Bird extends Animal {

	private String type;
	
	public Bird(String age, String color, int weight, int minimumWeight) {
		super(age, color, weight, minimumWeight);
		type = "Bird";
	}
	
	public int fly() {
		return (getWeight()-2);
	}
}
